  "devDependencies": {
    "c8": "^10.0.0",
    "eslint": "^8.19.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^6.0.0",
    "eslint-plugin-n": "^17.0.0",
    "mocha": "^11.0.0",
    "npm-run-all2": "^6.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8",
    "eslint-plugin-json": ">=3"
  },
  "dependencies": {
    "@humanwhocodes/momoa": "^3.0.2"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  }
