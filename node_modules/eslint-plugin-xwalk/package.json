{
  "name": "eslint-plugin-xwalk",
  "version": "0.1.1",
  "description": "Custom rules to validate aem-boilerplate-xwalk projects",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Adobe",
  "license": "Apache License 2.0",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
